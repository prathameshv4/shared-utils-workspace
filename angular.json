{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-secure-fetch": {
      "projectType": "library",
      "root": "projects/ng-secure-fetch",
      "sourceRoot": "projects/ng-secure-fetch/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-secure-fetch/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-secure-fetch/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/ng-secure-fetch/tsconfig.spec.json"
          }
        }
      }
    },
    "ngSecureFetchImplementation": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ng-secure-fetch-implementation",
      "sourceRoot": "projects/ng-secure-fetch-implementation/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/ng-secure-fetch-implementation/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/ng-secure-fetch-implementation/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/ng-secure-fetch-implementation/public"
              }
            ],
            "styles": [
              "projects/ng-secure-fetch-implementation/src/styles.scss"
            ],
            "server": "projects/ng-secure-fetch-implementation/src/main.server.ts",
            "outputMode": "server",
            "ssr": {
              "entry": "projects/ng-secure-fetch-implementation/src/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "ngSecureFetchImplementation:build:production"
            },
            "development": {
              "buildTarget": "ngSecureFetchImplementation:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/ng-secure-fetch-implementation/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/ng-secure-fetch-implementation/public"
              }
            ],
            "styles": [
              "projects/ng-secure-fetch-implementation/src/styles.scss"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
